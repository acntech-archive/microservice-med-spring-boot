<configuration>
    <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{dd.MM.yy | HH:mm:ss} | %-5p | %c | %m%n</pattern>
        </encoder>
    </appender>
    <root level="ERROR">
        <appender-ref ref="stdout"/>
    </root>
</configuration>